# Example .env for local testing and testnet deployments
# Do NOT commit actual private keys to source control.

# Private key for the deployer / owner (use a test account only)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# RPC endpoints
RPC_URL_SEPOLIA=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
RPC_URL_MAINNET=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# Dev wallet to receive fees (address)
DEV_WALLET=0xYourDevWalletAddress

# Optional: additional network RPCs
RPC_URL_BASE=
RPC_URL_ABSTRACT=

# Foundry: optional override for forge create
# FORGE_PRIVATE_KEY=0x...
